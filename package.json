  "scripts": {
    "build": "./build.js",
    "build:prod": "./build.js && yarn tailwindcss -i _css/input.css -o css/output.css --minify",
    "watch": "concurrently \"./build.js --watch\" \"bundle exec jekyll serve --livereload --incremental\" \"yarn tailwindcss -i _css/input.css -o css/output.css --minify --watch\"",
    "generate-icons": "node scripts/generate-icons.js",
    "update-icons": "git submodule update --remote assets/heroicons && yarn generate-icons"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "concurrently": "^9.2.0",
    "estrella": "^1.4.1",
    "tailwindcss": "^4.1.12"
  },
